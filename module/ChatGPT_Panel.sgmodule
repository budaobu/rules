#!name=ChatGPT Panel
#!desc=双重验证检测 (Trace + Server Status)，精准检测IP区域于风控。
#!category=Panel
#!arguments=icon:lasso.and.sparkles,iconerr:xmark.seal.fill,icon-color:#19C3AA,iconerr-color:#D65C51

[Panel]
ChatGPT = script-name=ChatGPT-Check, title="ChatGPT", content="刷新中...", update-interval=-1

[Script]
ChatGPT-Check = type=generic, timeout=8, script-path=https://raw.githubusercontent.com/budaobu/rules/refs/heads/main/module/ChatGPT_Panel.js, argument=icon={{{icon}}}&iconerr={{{iconerr}}}&icon-color={{{icon-color}}}&iconerr-color={{{iconerr-color}}}
